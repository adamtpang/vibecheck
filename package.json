{
    "name": "vibecheck-me",
    "version": "1.0.0",
    "description": "A Spotify playlist generator that creates your ultimate vibe playlist",
    "main": "server.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server.js",
        "client": "cd client && npm run dev",
        "build": "cd client && npm install && npm run build",
        "start": "node server.js"
    },
    "dependencies": {
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "dotenv": "^16.3.1",
        "spotify-web-api-node": "^5.0.2",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "concurrently": "^8.2.2"
    },
    "keywords": [
        "spotify",
        "playlist",
        "music",
        "vibe"
    ],
    "author": "Adam Pang",
    "license": "MIT"
}